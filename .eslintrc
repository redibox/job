{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "arrow-body-style": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "max-len": [1,150],
    "class-methods-use-this": 0,
    "no-continue": 0,
    "import/no-extraneous-dependencies": 0,
    "no-confusing-arrow": 0,
    "no-plusplus": 0
  },
  "globals": {
    "describe": true,
    "RediBox": true,
    "RediBoxCluster": true,
    "HOOK_NAME": true,
    "beforeEach": true,
    "Hook": true,
    "HookCluster": true,
    "it": true,
    "before": true,
    "after": true
  }
}
